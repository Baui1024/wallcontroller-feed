                    default-state = "off";
                    linux,default-trigger = "none";
                };

                led@6 {
                    label = "user:led6";
                    reg = <6>;
                    default-state = "off";
                    linux,default-trigger = "none";
                };

                led@7 {
                    label = "user:led7";
